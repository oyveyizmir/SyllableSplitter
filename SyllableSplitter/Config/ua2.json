{
  "LetterClasses": [
    "<я>=є,ї,ю,я",
    "<а>=е,і,у,а",
    "<г>=а,е,є,и,і,ї,о,у,ю,я",
    "<сон>=в,й,м,н,нь,л,ль,р,рь",
    "<гл>=п,т,ть,к,ш,с,сь,х,ч,ц,ць",
    "<дз>=б,д,дь,ґ,ж,з,зь,г,дж,дз,дзь"
  ],
  "Vowels": "<г>",
  "Consonants": "дзь,дж,дз,дь,зь,ль,нь,рь,сь,ть,ць,б,в,г,ґ,д,ж,з,й,к,л,м,н,п,р,с,т,ф,х,ц,ч,ш,щ",
  "Prefixes": "",
  "Separators": "-",
  "RewriteRules": [
    "'<я>/й<а>",
    "^<я>/й<а>",
    "<я>/й<а>/<г>_"
  ],
  "SplitRules": [
    "/(<дз><дз>|<гл><гл>)",
    "(<сон>|<дз>)/<гл>.*",
    "/(<гл>|<дз>)<сон>",
    "<сон>/<сон>",
    "/.+",
  ]
}
